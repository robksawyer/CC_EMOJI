var doc = fl.getDocumentDOM();if (!doc) {	alert("Please open or create a flashfile.");} else {	NameItRight();}function NameItRight(){	//doc.as3ExportFrame = 1; 	var libItems = doc.library.items;	var libItemsNum = libItems.length;	//var classesString = 'var '+doc.name.substr(0,doc.name.length-4)+'Classes = [';	var classes = [];		fl.outputPanel.clear();	fl.trace("Library Items to Rename: " + libItemsNum);		var baseClass = prompt('Choose a base class.', 'flash.display.BitmapData');	var baseClassSize = prompt('Input the size (Note: This is used to construct the name).', '20x20');		if (baseClass && baseClassSize)	{		var nClassName = "";		for (var i = 0; i < libItemsNum; i++) {			nClassName = "";			//Check to see if the Class has already been renamed.			if(libItems[i].linkageExportForAS){				if(libItems[i].linkageClassName){					nClassName = libItems[i].linkageClassName;					fl.trace("Reapplying ClassName " + libItems[i].linkageClassName + " for " + i + " -> " + libItems[i].name);					libItems[i].linkageExportInFirstFrame = true;					libItems[i].linkageBaseClass = baseClass;					libItems[i].linkageExportForAS = true;					libItems[i].linkageClassName = nClassName;				}			} else {				nClassName = "Emoji_" + baseClassSize + "_" + libItems[i].name.split(".png").join("");				fl.trace("Activating Export for " + i + " -> " + libItems[i].name);				libItems[i].linkageExportForAS = true;				libItems[i].linkageExportInFirstFrame = true;				fl.trace("Applying Class Type: " + baseClass);				libItems[i].linkageBaseClass = baseClass;				fl.trace("Adding ClassName " + nClassName + " to item #" + i);				libItems[i].linkageClassName = nClassName;			}		}	}	else	{		alert('No base class name or size set, library item(s) not changed');		return;	}}